{
  "name": "shellhax.desktop",
  "version": "1.0.0",
  "description": "Desktop Client For Shellshock.io/",
  "author": "Gaming Gurus Pty Ltd",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "Shellshock.io Client",
    "appId": "shellhax.desktop",
    "win": {
      "target": [
        "portable"
      ]
    },
    "mac": {
      "category": "public.app-category.games"
    },
    "portable": {
      "artifactName": "Shellshock.ioClient.exe"
    }
  },
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1"
  },
  "dependencies": {
    "bytenode": "^1.1.6",
    "debug": "^4.1.1",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.1.1",
    "electron-store": "^5.1.1",
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.2",
    "v8": "^0.1.0",
    "v8-compile-cache": "^2.1.0"
  }
}
